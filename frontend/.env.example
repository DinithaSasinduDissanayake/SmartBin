# SmartBin Frontend Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:5000/api

# Stripe Configuration (for payment integration)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Feature Flags (optional, for toggling features during development)
VITE_ENABLE_MFA=true
VITE_ENABLE_STRIPE=true

# Analytics (optional, for production)
# VITE_ANALYTICS_ID=your-analytics-id